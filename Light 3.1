int led = D6;
int lightSensor = A1;
int analogValue = 0;
void setup() {
pinMode(led, OUTPUT);
pinMode(lightSensor, INPUT);

}

void loop() {
analogValue = analogRead(lightSensor);
digitalWrite(led, HIGH);
String intensity = String (analogValue);
Particle.publish("Light" , intensity , PRIVATE);
delay(5000);
digitalWrite(led, LOW);
delay(5000);
}
